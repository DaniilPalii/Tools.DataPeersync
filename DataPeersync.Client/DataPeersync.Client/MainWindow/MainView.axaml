<UserControl 
	xmlns="https://github.com/avaloniaui"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:local="clr-namespace:DataPeersync.Client.MainWindow"
	x:Class="DataPeersync.Client.MainWindow.MainView"
	x:DataType="local:MainViewModel"
	Foreground="White"
	Background="#171C2C"
>
	<Design.DataContext>
		<local:MainViewModel />
	</Design.DataContext>
	
	<ContentControl Content="{Binding Content}"	/>
</UserControl>
